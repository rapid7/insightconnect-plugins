Name = 'Netmiko'
Vendor = 'rapid7'
Version = '1.0.1'
Description = 'Netmiko is used to access network devices over SSH'


class ICONNetmiko(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.ConfigurationCommands())

        self.add_action(actions.ShowCommands())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONNetmiko())
    cli.run()


