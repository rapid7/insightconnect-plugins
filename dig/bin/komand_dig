#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_dig import connection, actions, triggers


Name = 'Dig'
Vendor = 'rapid7'
Version = '1.0.2'
Description = 'Dig is used for forward and reverse DNS lookups'


class ICONDig(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Forward())

        self.add_action(actions.Reverse())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONDig())
    cli.run()


if __name__ == "__main__":
    main()
