#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_threatq import connection, actions, triggers


Name = "Threat Quotient"
Vendor = "rapid7"
Version = "1.0.2"
Description = "The ThreatQ plugin provides ability to work with Threat Quotient indicators and events"


class ICONThreatq(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.CreateEvent())

        self.add_action(actions.CreateIndicator())

        self.add_action(actions.GetEvent())

        self.add_action(actions.GetIndicator())

        self.add_action(actions.Search())

        self.add_action(actions.SearchEvents())

        self.add_action(actions.SearchIndicators())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONThreatq())
    cli.run()


if __name__ == "__main__":
    main()
