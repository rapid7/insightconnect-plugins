#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_csv import connection, actions, triggers


Name = 'CSV'
Vendor = 'rapid7'
Version = '1.1.4'
Description = 'CSV (Comma Separated Values) is a simple data format for storing data. This plugin allows one to extract fields from CSV strings and files'


class ICONCsv(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.FilterBytes())

        self.add_action(actions.FilterString())

        self.add_action(actions.JsonToCsv())

        self.add_action(actions.ToJson())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONCsv())
    cli.run()


if __name__ == "__main__":
    main()
