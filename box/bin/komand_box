#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_box import connection, actions, triggers


Name = 'Box'
Vendor = 'rapid7'
Version = '2.0.4'
Description = 'Box gives you access to secure content management and content experience features of the Box Enterprise API'


class ICONBox(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.CreateUser())

        self.add_action(actions.DeleteFile())

        self.add_action(actions.DeleteFolder())

        self.add_action(actions.DeleteUser())

        self.add_action(actions.DownloadFile())

        self.add_action(actions.GetFile())

        self.add_action(actions.GetFolder())

        self.add_action(actions.LockFile())

        self.add_action(actions.UnlockFile())

        self.add_action(actions.UploadFile())

        self.add_action(actions.UserInfo())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONBox())
    cli.run()


if __name__ == "__main__":
    main()
