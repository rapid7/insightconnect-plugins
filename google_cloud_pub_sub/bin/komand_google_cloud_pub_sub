import komand
from komand_google_cloud_pub_sub import connection, actions, triggers


Name = 'Google Cloud Pub Sub'
Vendor = 'rapid7'
Version = '3.1.2'
Description = 'A fully-managed real-time messaging service that allows you to send and receive messages'


class ICONGoogleCloudPubSub(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_trigger(triggers.Subscription())

        self.add_action(actions.CreateSubscription())

        self.add_action(actions.CreateTopic())

        self.add_action(actions.ListTopics())

        self.add_action(actions.Publish())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONGoogleCloudPubSub())
    cli.run()


if __name__ == "__main__":
    main()
