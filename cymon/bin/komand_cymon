#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_cymon import connection, actions, triggers


Name = 'Cymon'
Vendor = 'rapid7'
Version = '1.0.2'
Description = 'Cymon Open Threat Intelligence'


class ICONCymon(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_trigger(triggers.PollAddressBlacklist())

        self.add_trigger(triggers.PollDomainBlacklist())

        self.add_action(actions.AddressBlacklist())

        self.add_action(actions.AddressLookup())

        self.add_action(actions.DomainBlacklist())

        self.add_action(actions.DomainLookup())

        self.add_action(actions.UrlLookup())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONCymon())
    cli.run()


if __name__ == "__main__":
    main()
