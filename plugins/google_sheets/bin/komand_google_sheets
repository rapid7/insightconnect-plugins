#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_google_sheets import connection, actions, triggers


Name = 'Google Sheets'
Vendor = 'rapid7'
Version = '1.1.2'
Description = 'Create, edit and collaborate with others on spreadsheets'


class ICONGoogleSheets(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.SpreadListToSheet())

        self.add_action(actions.UpdateCell())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONGoogleSheets())
    cli.run()


if __name__ == "__main__":
    main()
