#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_openphish import connection, actions, triggers


Name = 'OpenPhish'
Vendor = 'rapid7'
Version = '1.0.1'
Description = 'Identify zero-day phishing sites and provide real-time threat intelligence'


class ICONOpenphish(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_trigger(triggers.SaveFeedFile())

        self.add_action(actions.CheckUrlReputation())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONOpenphish())
    cli.run()


if __name__ == "__main__":
    main()
