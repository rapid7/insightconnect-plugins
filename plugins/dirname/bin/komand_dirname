import komand
from komand_dirname import connection, actions, triggers


Name = "Dirname"
Vendor = "rapid7"
Version = "1.0.3"
Description = "This plugin gets the directory name of a file path or protocol and domain of a URL"


class ICONDirname(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Dirname())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONDirname())
    cli.run()


