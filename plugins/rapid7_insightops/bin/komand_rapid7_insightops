import komand
from komand_rapid7_insightops import connection, actions, triggers


Name = 'Rapid7 InsightOps'
Vendor = 'rapid7'
Version = '1.0.2'
Description = 'This plugin allows you to submit and query InsightOps logs'


class ICONRapid7Insightops(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.CreateLogsetContainer())

        self.add_action(actions.QueryLogs())

        self.add_action(actions.SubmitLogData())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONRapid7Insightops())
    cli.run()


if __name__ == "__main__":
    main()
