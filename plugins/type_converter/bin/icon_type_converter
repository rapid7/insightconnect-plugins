#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import os
import json
from sys import argv

Name = "Type Converter"
Vendor = "rapid7"
Version = "1.8.1"
Description = "Type Converter is a utility plugin for converting data types within a Rapid7 InsightConnect workflow"


def main():
    if 'http' in argv:
        if os.environ.get("GUNICORN_CONFIG_FILE"):
            with open(os.environ.get("GUNICORN_CONFIG_FILE")) as gf:
                gunicorn_cfg = json.load(gf)
                if gunicorn_cfg.get("worker_class", "sync") == "gevent":
                    from gevent import monkey
                    monkey.patch_all()
        elif 'gevent' in argv:
            from gevent import monkey
            monkey.patch_all()

    import insightconnect_plugin_runtime
    from icon_type_converter import connection, actions, triggers

    class ICONTypeConverter(insightconnect_plugin_runtime.Plugin):
        def __init__(self):
            super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
            )
            self.add_action(actions.ArrayDiff())

            self.add_action(actions.ArrayMatch())

            self.add_action(actions.ArrayToString())

            self.add_action(actions.BooleanToInteger())

            self.add_action(actions.BooleanToString())

            self.add_action(actions.CombineArrays())

            self.add_action(actions.IntegerToBoolean())

            self.add_action(actions.IntegerToNumber())

            self.add_action(actions.IntegerToString())

            self.add_action(actions.NumberToInteger())

            self.add_action(actions.ObjectToString())

            self.add_action(actions.StringToBoolean())

            self.add_action(actions.StringToFloat())

            self.add_action(actions.StringToInteger())

            self.add_action(actions.StringToList())

            self.add_action(actions.StringToObject())


    """Run plugin"""
    cli = insightconnect_plugin_runtime.CLI(ICONTypeConverter())
    cli.run()


if __name__ == "__main__":
    main()
