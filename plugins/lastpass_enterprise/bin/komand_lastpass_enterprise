import komand
from komand_lastpass_enterprise import connection, actions, triggers


Name = 'LastPass Enterprise'
Vendor = 'rapid7'
Version = '1.0.2'
Description = 'A tool for conveniently and securely managing passwords'


class ICONLastpassEnterprise(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.ChangeGroup())

        self.add_action(actions.DeprovisionUser())

        self.add_action(actions.ProvisionUser())


