#mirrorlist=https://mirrors.fedoraproject.org/metalink?repo=updates-released-debug-f24&arch=$arch
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-7

[fedora-updates-source]
name=Fedora 24 - Updates Source
failovermethod=priority
#baseurl=http://download.fedoraproject.org/pub/fedora/linux/updates/24/SRPMS/
