import komand
from komand_cymon_v2 import connection, actions, triggers


Name = 'Cymon v2'
Vendor = 'rapid7'
Version = '1.0.2'
Description = 'Cymon is the largest tracker of open-source security reports about phishing, malware, botnets and other malicious activities'


class ICONCymonV2(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.CreateFeed())

        self.add_action(actions.GetFeedDetails())

        self.add_action(actions.GetReportDocument())

        self.add_action(actions.ListAllFeeds())

        self.add_action(actions.ListUserFeeds())

        self.add_action(actions.Search())

        self.add_action(actions.SubmitReport())

        self.add_action(actions.SubmitReportsInBulk())

        self.add_action(actions.UpdateFeed())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONCymonV2())
    cli.run()


if __name__ == "__main__":
    main()
