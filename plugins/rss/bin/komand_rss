import komand
from komand_rss import connection, actions, triggers


Name = "RSS"
Vendor = "rapid7"
Version = "1.0.3"
Description = "The RSS plugin allows you to monitor an RSS feed"


class ICONRss(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_trigger(triggers.Poll())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONRss())
    cli.run()


if __name__ == "__main__":
    main()
