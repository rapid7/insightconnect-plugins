#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_eml import connection, actions, triggers


Name = 'EML'
Vendor = 'rapid7'
Version = '1.1.2'
Description = 'The EML plugins will parse and extract data from an EML file'


class ICONEml(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Parse())

        self.add_action(actions.ParseFromString())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONEml())
    cli.run()


if __name__ == "__main__":
    main()
