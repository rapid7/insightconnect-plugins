#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_threatstack import connection, actions, triggers


Name = 'Threat Stack'
Vendor = 'rapid7'
Version = '1.0.0'
Description = 'The Threat Stack plugin is used to get information about alerts, assets, and policies'


class ICONThreatstack(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.GetAgent())

        self.add_action(actions.GetAgents())

        self.add_action(actions.GetAlert())

        self.add_action(actions.GetAlerts())

        self.add_action(actions.GetOrganization())

        self.add_action(actions.GetOrganizations())

        self.add_action(actions.GetPolicies())

        self.add_action(actions.GetPolicy())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONThreatstack())
    cli.run()


if __name__ == "__main__":
    main()
