#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_request_tracker import connection, actions, triggers


Name = 'Request Tracker'
Vendor = 'rapid7'
Version = '1.0.1'
Description = 'The Request Tracker plugin allows you to create and manage tickets in Request Tracker'


class ICONRequestTracker(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.TicketAttachment())

        self.add_action(actions.TicketAttachmentContent())

        self.add_action(actions.TicketAttachments())

        self.add_action(actions.TicketCreate())

        self.add_action(actions.TicketHistory())

        self.add_action(actions.TicketHistoryEntry())

        self.add_action(actions.TicketLinks())

        self.add_action(actions.TicketProperties())

        self.add_action(actions.TicketSearch())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONRequestTracker())
    cli.run()


if __name__ == "__main__":
    main()
