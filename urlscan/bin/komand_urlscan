#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_urlscan import connection, actions, triggers


Name = 'urlscan.io'
Vendor = 'rapid7'
Version = '2.1.3'
Description = 'The URLScan plugin uses URLScan to analyze urls for malicious indicators'


class ICONUrlscan(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.GetScanResults())

        self.add_action(actions.Search())

        self.add_action(actions.SubmitUrlForScan())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONUrlscan())
    cli.run()


if __name__ == "__main__":
    main()
