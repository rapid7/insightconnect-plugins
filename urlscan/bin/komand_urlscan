#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_urlscan import connection, actions, triggers


Name = "urlscan.io"
Vendor = "rapid7"
Version = "2.1.4"
Description = "Analyze URLs for malicious indicators using the URLScan website scanner"


class ICONUrlscan(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.GetScanResults())

        self.add_action(actions.Search())

        self.add_action(actions.SubmitUrlForScan())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONUrlscan())
    cli.run()


if __name__ == "__main__":
    main()
