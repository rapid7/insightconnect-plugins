#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_grep import connection, actions, triggers


Name = 'Grep'
Vendor = 'rapid7'
Version = '1.0.3'
Description = 'This plugin searches for a specified pattern in a string or a file'


class ICONGrep(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.MatchBase64())

        self.add_action(actions.MatchString())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONGrep())
    cli.run()


if __name__ == "__main__":
    main()
