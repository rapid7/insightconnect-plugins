#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_qradar import connection, actions, triggers


Name = 'QRadar Security Intelligence Platform'
Vendor = 'rapid7'
Version = '2.0.1'
Description = 'Run Ariel queries and retrieve policy offense data'


class ICONQradar(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.AddDataToReferenceDataLists())

        self.add_action(actions.GetArielQueryResults())

        self.add_action(actions.QueryDataWithAriel())

        self.add_action(actions.RetrieveOffenses())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONQradar())
    cli.run()


if __name__ == "__main__":
    main()
