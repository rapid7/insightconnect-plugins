#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_wordpress import connection, actions, triggers


Name = 'WordPress'
Vendor = 'rapid7'
Version = '1.0.1'
Description = 'The WordPress plugin allows for user management on the WordPress platform'


class ICONWordpress(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.DeleteUser())

        self.add_action(actions.SuspendUser())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONWordpress())
    cli.run()


if __name__ == "__main__":
    main()
