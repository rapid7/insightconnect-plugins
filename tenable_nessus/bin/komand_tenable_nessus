#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_tenable_nessus import connection, actions, triggers


Name = 'Tenable Nessus'
Vendor = 'rapid7'
Version = '2.0.0'
Description = 'Identify the vulnerabilities, policy-violating configurations and malware that attackers could use to penetrate your network'


class ICONTenableNessus(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.CreateScan())

        self.add_action(actions.DownloadReport())

        self.add_action(actions.GetScans())

        self.add_action(actions.LaunchScan())

        self.add_action(actions.ListScanners())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONTenableNessus())
    cli.run()


if __name__ == "__main__":
    main()
