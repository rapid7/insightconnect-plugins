#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_duo_admin import connection, actions, triggers


Name = 'Duo Admin API'
Vendor = 'rapid7'
Version = '3.3.2'
Description = 'Duo is a trusted access solution for organizations. The Duo Admin plugin for Rapid7 InsightConnect allows users to manage and administrate their Duo organization'


class ICONDuoAdmin(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.AddUser())

        self.add_action(actions.DeleteUser())

        self.add_action(actions.EnrollUser())

        self.add_action(actions.GetLogs())

        self.add_action(actions.GetPhonesByUserId())

        self.add_action(actions.GetUserById())

        self.add_action(actions.GetUserByUsername())

        self.add_action(actions.GetUserStatus())

        self.add_action(actions.GetUsers())

        self.add_action(actions.ModifyUser())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONDuoAdmin())
    cli.run()


if __name__ == "__main__":
    main()
