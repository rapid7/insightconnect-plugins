import komand
from icon_kintone import connection, actions, triggers


Name = 'Kintone'
Vendor = 'rapid7'
Version = '1.0.1'
Description = 'Custom workflows and data management for businesses and non-profits in one place'


class ICONKintone(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name, 
                vendor=Vendor, 
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.GetRecordById())

        self.add_action(actions.WriteRecord())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONKintone())
    cli.run()


if __name__ == "__main__":
    main()
