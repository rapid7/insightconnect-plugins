import komand
from komand_port_knocking import connection, actions, triggers


Name = 'Port Knocking'
Vendor = 'rapid7'
Version = '1.0.1'
Description = 'Knock the specified ports on a host'


class ICONPortKnocking(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Knock())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONPortKnocking())
    cli.run()


