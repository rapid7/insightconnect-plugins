#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_github_enterprise import connection, actions, triggers


Name = 'GitHub Enterprise'
Vendor = 'rapid7'
Version = '1.0.0'
Description = 'GitHub Enterprise plugin allows user management'


class ICONGithubEnterprise(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_trigger(triggers.Issue())

        self.add_action(actions.Demote())

        self.add_action(actions.Organization())

        self.add_action(actions.Promote())

        self.add_action(actions.Suspend())

        self.add_action(actions.Unsuspend())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONGithubEnterprise())
    cli.run()


if __name__ == "__main__":
    main()
