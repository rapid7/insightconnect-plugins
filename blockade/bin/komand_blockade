#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_blockade import connection, actions, triggers


Name = 'Blockade'
Vendor = 'rapid7'
Version = '1.0.0'
Description = 'Blockade brings antivirus-like capabilities to users who run the Chrome browser'


class ICONBlockade(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.AddIndicators())

        self.add_action(actions.AddUser())

        self.add_action(actions.GetEvents())

        self.add_action(actions.GetIndicators())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONBlockade())
    cli.run()


if __name__ == "__main__":
    main()
