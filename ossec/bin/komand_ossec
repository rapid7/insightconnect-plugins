#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_ossec import connection, actions, triggers


Name = 'OSSEC'
Vendor = 'rapid7'
Version = '1.0.0'
Description = 'OSSEC is a free, open-source host-based intrusion detection system. Using the OSSEC plugin for InsightConnect, users can parse OSSEC alerts into JSON format'


class ICONOssec(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.AlertParse())

        self.add_action(actions.RootcheckParse())

        self.add_action(actions.SyscheckParse())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONOssec())
    cli.run()


if __name__ == "__main__":
    main()
