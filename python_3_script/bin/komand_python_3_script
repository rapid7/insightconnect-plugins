from komand_python_3_script import connection, actions, triggers


Name = "Python 3 Script"
Vendor = "rapid7"
Version = "2.0.2"
Description = "Run a Python 3 script"


class ICONPython3Script(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Run())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONPython3Script())
    cli.run()


