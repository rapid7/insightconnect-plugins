#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_typo_squatter import connection, actions, triggers


Name = 'Typo Squatter'
Vendor = 'rapid7'
Version = '1.0.1'
Description = 'Typo Squatter detects cybersquatting of domains and allows for domain scoring. This plugin can be used to aid in phishing investigation and analysis'


class ICONTypoSquatter(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_trigger(triggers.SearchCertstream())

        self.add_action(actions.CheckForSquatters())

        self.add_action(actions.ScoreDomain())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONTypoSquatter())
    cli.run()


if __name__ == "__main__":
    main()
