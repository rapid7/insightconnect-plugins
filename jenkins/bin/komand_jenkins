#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_jenkins import connection, actions, triggers


Name = 'Jenkins'
Vendor = 'rapid7'
Version = '1.1.1'
Description = 'Reliably build, test, and deploy their software'


class ICONJenkins(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.BuildInfo())

        self.add_action(actions.BuildJob())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONJenkins())
    cli.run()


if __name__ == "__main__":
    main()
