#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_grafana import connection, actions, triggers


Name = 'Grafana'
Vendor = 'rapid7'
Version = '1.0.1'
Description = 'The Grafana plugin allows you to manage users in your Grafana organization'


class ICONGrafana(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.AddOrganizationUser())

        self.add_action(actions.DeleteGlobalUser())

        self.add_action(actions.DeleteOrganizationUser())

        self.add_action(actions.DoProxiedDatasourceCall())

        self.add_action(actions.GetOrganizationUsers())

        self.add_action(actions.GetUser())

        self.add_action(actions.SearchUsers())

        self.add_action(actions.UpdateOrganization())

        self.add_action(actions.UpdateOrganizationUser())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONGrafana())
    cli.run()


if __name__ == "__main__":
    main()
