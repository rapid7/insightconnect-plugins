#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from icon_google_docs import connection, actions, triggers


Name = 'Google Docs'
Vendor = 'rapid7'
Version = '1.0.0'
Description = 'Create and retrieve Google documents'


class ICONGoogleDocs(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name, 
                vendor=Vendor, 
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.CreateBlankDocument())

        self.add_action(actions.CreateDocument())

        self.add_action(actions.GetDocument())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONGoogleDocs())
    cli.run()


if __name__ == "__main__":
    main()
