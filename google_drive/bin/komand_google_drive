#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_google_drive import connection, actions, triggers


Name = 'Google Drive'
Vendor = 'rapid7'
Version = '2.1.2'
Description = 'Upload and retrieve files from Google drive'


class ICONGoogleDrive(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.FindFileByName())

        self.add_action(actions.GetFileContents())

        self.add_action(actions.OverwriteFile())

        self.add_action(actions.UploadFile())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONGoogleDrive())
    cli.run()


if __name__ == "__main__":
    main()
