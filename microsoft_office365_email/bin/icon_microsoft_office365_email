#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from icon_microsoft_office365_email import connection, actions, triggers


Name = 'Microsoft Office 365 Email'
Vendor = 'rapid7'
Version = '4.0.1'
Description = 'Microsoft Office 365 message actions and triggers'


class ICONMicrosoftOffice365Email(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name, 
                vendor=Vendor, 
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_trigger(triggers.EmailReceived())

        self.add_action(actions.DeleteEmail())

        self.add_action(actions.GetEmailFromUser())

        self.add_action(actions.MoveEmail())

        self.add_action(actions.SendEmail())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONMicrosoftOffice365Email())
    cli.run()


if __name__ == "__main__":
    main()
