#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_freeipa import connection, actions, triggers


Name = 'FreeIPA'
Vendor = 'rapid7'
Version = '2.0.1'
Description = 'This plugin can retrieve user details and statuses and delete users from FreeIPA servers'


class ICONFreeipa(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.DeleteUser())

        self.add_action(actions.FindUser())

        self.add_action(actions.ShowUser())

        self.add_action(actions.UserStatus())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONFreeipa())
    cli.run()


if __name__ == "__main__":
    main()
