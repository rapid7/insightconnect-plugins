import komand
from icon_advanced_regex import connection, actions, triggers


Name = 'Advanced Regex'
Vendor = 'rapid7'
Version = '1.0.0'
Description = 'This plugin performs advanced regex operations on a string using Python regex'


class ICONAdvancedRegex(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name, 
                vendor=Vendor, 
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.DataExtraction())

        self.add_action(actions.Replace())

        self.add_action(actions.Split())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONAdvancedRegex())
    cli.run()


if __name__ == "__main__":
    main()
