import komand
from komand_python_2_script import connection, actions, triggers


Name = 'Python 2 Script'
Vendor = 'rapid7'
Version = '2.0.3'
Description = 'The Python 2 plugin allows you to run a Python 2 script'


class ICONPython2Script(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Run())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONPython2Script())
    cli.run()


if __name__ == "__main__":
    main()
