import komand
from komand_ssh import connection, actions, triggers


Name = 'SSH'
Vendor = 'rapid7'
Version = '1.0.2'
Description = 'The SSH plugin allows you to run commands on a remote host'


class ICONSsh(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Run())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONSsh())
    cli.run()


