#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_abuseipdb import connection, actions, triggers


Name = 'AbuseIPDB'
Vendor = 'rapid7'
Version = '5.0.0'
Description = 'AbuseIPDB is a free service which allows you to look up IP reports, or report an abusive IP'


class ICONAbuseipdb(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.CheckCidr())

        self.add_action(actions.CheckIp())

        self.add_action(actions.GetBlacklist())

        self.add_action(actions.ReportIp())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONAbuseipdb())
    cli.run()


if __name__ == "__main__":
    main()
