#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_grr import connection, actions, triggers


Name = 'Google Rapid Response'
Vendor = 'rapid7'
Version = '2.0.1'
Description = 'The GRR plugin allows you to organize and start hunts using GRR'


class ICONGrr(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Hunting())

        self.add_action(actions.Labeling())

        self.add_action(actions.Listing())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONGrr())
    cli.run()


if __name__ == "__main__":
    main()
