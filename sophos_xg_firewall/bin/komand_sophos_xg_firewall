import komand
from komand_sophos_xg_firewall import connection, actions, triggers


Name = 'Sophos XG Firewall'
Vendor = 'rapid7'
Version = '2.0.0'
Description = 'The Sophos XG Firewall plugin allows you to manage your Sophos XG Firewall through policies'


class ICONSophosXgFirewall(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.CreateNetworkpolicy())

        self.add_action(actions.CreateNonhttpbasedpolicy())

        self.add_action(actions.CreatePublicnonhttpbasedpolicy())

        self.add_action(actions.CreateUserpolicy())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONSophosXgFirewall())
    cli.run()


if __name__ == "__main__":
    main()
