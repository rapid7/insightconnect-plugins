#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_tenable_io import connection, actions, triggers


Name = "TenableIO"
Vendor = "rapid7"
Version = "1.0.1"
Description = "The TenableIO plugin allows you to launch scans and analyze reports"


class ICONTenableIo(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.DownloadReport())

        self.add_action(actions.LaunchScan())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONTenableIo())
    cli.run()


if __name__ == "__main__":
    main()
