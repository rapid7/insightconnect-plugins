#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_zeus_tracker import connection, actions, triggers


Name = 'ZeuS Tracker'
Vendor = 'rapid7'
Version = '1.0.2'
Description = 'The ZeuS Command&Control Server Tracking plugin'


class ICONZeusTracker(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Asn())

        self.add_action(actions.Binaries())

        self.add_action(actions.Configs())

        self.add_action(actions.Countries())

        self.add_action(actions.Dropzones())

        self.add_action(actions.Host())

        self.add_action(actions.IpAddress())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONZeusTracker())
    cli.run()


if __name__ == "__main__":
    main()
