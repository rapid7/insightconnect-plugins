import komand
from komand_ipinfo import connection, actions, triggers


Name = 'IPinfo'
Vendor = 'rapid7'
Version = '1.0.2'
Description = 'Query IP Address Information'


class ICONIpinfo(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.IpLookup())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONIpinfo())
    cli.run()


