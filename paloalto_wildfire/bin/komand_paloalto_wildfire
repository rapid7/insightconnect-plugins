Version = '1.1.1'
Description = 'Palo Alto Wildfire is a cloud-based threat analysis service. Perform malware analysis with the Palo Alto Wildfire plugin for InsightConnect malware'


class ICONPaloaltoWildfire(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.GetMalwareTestFile())

        self.add_action(actions.GetPcap())

        self.add_action(actions.GetReport())

        self.add_action(actions.GetSample())

        self.add_action(actions.GetVerdict())

        self.add_action(actions.SubmitFile())

        self.add_action(actions.SubmitFileFromUrl())

        self.add_action(actions.SubmitUrl())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONPaloaltoWildfire())
    cli.run()


if __name__ == "__main__":
    main()
