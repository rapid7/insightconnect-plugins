#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_trello import connection, actions, triggers


Name = 'Trello'
Vendor = 'rapid7'
Version = '2.0.2'
Description = 'Manage members and their permissions in your Trello organization'


class ICONTrello(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.DeactivateUser())

        self.add_action(actions.DeactivatedList())

        self.add_action(actions.GetBoardsByMember())

        self.add_action(actions.MemberList())

        self.add_action(actions.RemoveMemberFromBoard())

        self.add_action(actions.RemoveMemberFromCards())

        self.add_action(actions.RemoveMemberFromOrg())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONTrello())
    cli.run()


if __name__ == "__main__":
    main()
