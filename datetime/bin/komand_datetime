#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import insightconnect_plugin_runtime
from komand_datetime import connection, actions, triggers


Name = "Datetime"
Vendor = "rapid7"
Version = "2.0.6"
Description = "This plugin manipulates timestamps using Python's maya library"


class ICONDatetime(insightconnect_plugin_runtime.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.AddToDatetime())

        self.add_action(actions.DateFromEpoch())

        self.add_action(actions.EpochFromDate())

        self.add_action(actions.GetDatetime())

        self.add_action(actions.SubtractFromDatetime())

        self.add_action(actions.TimeElapsed())


def main():
    """Run plugin"""
    cli = insightconnect_plugin_runtime.CLI(ICONDatetime())
    cli.run()


if __name__ == "__main__":
    main()
