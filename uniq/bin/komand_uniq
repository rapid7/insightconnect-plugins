#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_uniq import connection, actions, triggers


Name = 'Uniq'
Vendor = 'rapid7'
Version = '1.0.2'
Description = 'Uniq removes redundant lines or items from data. Using the Uniq plugin can allow users to deduplicate datasets or items prior to processing within a workflow'


class ICONUniq(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.UniqIntegerArray())

        self.add_action(actions.UniqStringArray())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONUniq())
    cli.run()


if __name__ == "__main__":
    main()
