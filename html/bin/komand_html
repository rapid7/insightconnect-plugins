#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_html import connection, actions, triggers


Name = 'HTML'
Vendor = 'rapid7'
Version = '1.0.1'
Description = 'Validate or convert an html document'


class ICONHtml(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Docx())

        self.add_action(actions.Epub())

        self.add_action(actions.Html5())

        self.add_action(actions.Markdown())

        self.add_action(actions.Pdf())

        self.add_action(actions.Validate())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONHtml())
    cli.run()


if __name__ == "__main__":
    main()
