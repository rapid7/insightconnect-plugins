import komand
from komand_crits import connection, actions, triggers


Name = 'CRITs'
Vendor = 'rapid7'
Version = '1.0.1'
Description = 'CRITs is an open source malware and threat repository for conducting malware analyses. With the CRITs plugin for Rapid7 InsightConnect, users can manage actors, events, collections, domains, and more'


class ICONCrits(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.AddActor())

        self.add_action(actions.AddActorIdentifier())

        self.add_action(actions.AddCampaign())

        self.add_action(actions.AddCertificate())

        self.add_action(actions.AddDomain())

        self.add_action(actions.AddEmail())

        self.add_action(actions.AddEvent())

        self.add_action(actions.AddIndicator())

        self.add_action(actions.AddIp())

        self.add_action(actions.AddPcap())

        self.add_action(actions.AddRawData())

        self.add_action(actions.AddSample())

        self.add_action(actions.AddTarget())

        self.add_action(actions.GetCollection())

        self.add_action(actions.GetItem())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONCrits())
    cli.run()


if __name__ == "__main__":
    main()
