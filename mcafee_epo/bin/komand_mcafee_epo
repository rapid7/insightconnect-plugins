from komand_mcafee_epo import connection, actions, triggers


Name = "McAfee ePO"
Vendor = "rapid7"
Version = "3.1.0"
Description = "McAfee ePolicy Orchestrator provides a web API for McAfee endpoint protection management activities"


class ICONMcafeeEpo(insightconnect_plugin_runtime.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.AddPermissionSetToUser())

        self.add_action(actions.ClearTags())

        self.add_action(actions.GetPolicies())

        self.add_action(actions.RunWakeUp())

        self.add_action(actions.SearchAgents())

        self.add_action(actions.TagSystem())


def main():
    """Run plugin"""
    cli = insightconnect_plugin_runtime.CLI(ICONMcafeeEpo())
    cli.run()


if __name__ == "__main__":
    main()
