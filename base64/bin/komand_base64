#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_base64 import connection, actions, triggers


Name = 'Base64'
Vendor = 'rapid7'
Version = '1.1.2'
Description = 'Encode and decode data using the base64 alphabet'


class ICONBase64(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Decode())

        self.add_action(actions.Encode())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONBase64())
    cli.run()


if __name__ == "__main__":
    main()
