#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_office365_admin import connection, actions, triggers


Name = 'Office365 Admin'
Vendor = 'rapid7'
Version = '1.3.0'
Description = 'The Office 365 Admin plugin allows you to manage users in Office 365'


class ICONOffice365Admin(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.AddUser())

        self.add_action(actions.AssignLicenseToUser())

        self.add_action(actions.DeleteUser())

        self.add_action(actions.GetSubscribedSkus())

        self.add_action(actions.UpdateUsageLocation())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONOffice365Admin())
    cli.run()


if __name__ == "__main__":
    main()
