import komand
from komand_math import connection, actions, triggers


Name = 'Math'
Vendor = 'rapid7'
Version = '1.1.3'
Description = 'Basic mathematical operations'


class ICONMath(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Calculate())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONMath())
    cli.run()


