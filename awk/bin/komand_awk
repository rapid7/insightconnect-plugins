#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_awk import connection, actions, triggers


Name = 'Awk'
Vendor = 'rapid7'
Version = '1.2.1'
Description = 'This plugin manipulates an input string or file with the GNU awk programming language'


class ICONAwk(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.ProcessFile())

        self.add_action(actions.ProcessString())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONAwk())
    cli.run()


if __name__ == "__main__":
    main()
