#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_passivetotal import connection, actions, triggers


Name = 'PassiveTotal'
Vendor = 'rapid7'
Version = '1.0.1'
Description = 'Use PassiveTotal to analyze domains, addresses, and WHOIS data'


class ICONPassivetotal(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_trigger(triggers.ProjectUpdated())

        self.add_action(actions.GetMonitorAlerts())

        self.add_action(actions.GetSubdomains())

        self.add_action(actions.GetWhois())

        self.add_action(actions.HistorySsl())

        self.add_action(actions.LookupAddress())

        self.add_action(actions.LookupAddresses())

        self.add_action(actions.LookupDomain())

        self.add_action(actions.LookupDomains())

        self.add_action(actions.LookupSsl())

        self.add_action(actions.SearchWhois())

        self.add_action(actions.SearchWhoisByKeyword())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONPassivetotal())
    cli.run()


if __name__ == "__main__":
    main()
