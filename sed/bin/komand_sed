#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_sed import connection, actions, triggers


Name = "Sed"
Vendor = "rapid7"
Version = "2.0.2"
Description = "The Sed plugin allows you to run the GNU stream editor on strings and files"


class ICONSed(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.ProcessBytes())

        self.add_action(actions.ProcessString())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONSed())
    cli.run()


if __name__ == "__main__":
    main()
