#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_tsv import connection, actions, triggers


Name = "TSV"
Vendor = "rapid7"
Version = "1.0.1"
Description = "Extract fields from TSV-formatted strings and files"


class ICONTsv(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.FilterBytes())

        self.add_action(actions.FilterString())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONTsv())
    cli.run()


if __name__ == "__main__":
    main()
