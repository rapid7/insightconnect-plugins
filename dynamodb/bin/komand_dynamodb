import komand
from komand_dynamodb import connection, actions, triggers


Name = 'Dynamo DB'
Vendor = 'rapid7'
Version = '1.0.1'
Description = 'Dynamo DB is a Key/Value store powered by Amazon available for members of AWS'


class ICONDynamodb(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Insert())

        self.add_action(actions.Scan())

        self.add_action(actions.Update())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONDynamodb())
    cli.run()


if __name__ == "__main__":
    main()
