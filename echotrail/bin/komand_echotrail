Name = 'EchoTrail Insights'
Vendor = 'echotrail'
Version = '1.0.1'
Description = 'EchoTrail Insights is a database of executable behavioral analytics, searchable by filename or SHA256 or MD5 hash. Users can lookup filenames and hashes using the EchoTrail Insights plugin for Rapid7 InsightConnect'


class ICONEchotrail(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.FilenameLookup())

        self.add_action(actions.HashLookup())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONEchotrail())
    cli.run()


if __name__ == "__main__":
    main()
