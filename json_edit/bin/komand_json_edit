import komand
from komand_json_edit import connection, actions, triggers


Name = 'JSON Edit'
Vendor = 'rapid7'
Version = '1.0.2'
Description = 'Provides the ability to edit JSON data'


class ICONJsonEdit(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Delete())

        self.add_action(actions.Update())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONJsonEdit())
    cli.run()


if __name__ == "__main__":
    main()
