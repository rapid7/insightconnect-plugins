import komand
from komand_cuckoo import connection, actions, triggers


Name = 'Cuckoo Sandbox'
Vendor = 'rapid7'
Version = '1.0.2'
Description = 'Cuckoo Sandbox is an open source automated malware analysis system. Using the Cuckoo Sandbox plugin for Rapid7 InsightConnect, users can analyze files and URLs, manage tasks, and more'


class ICONCuckoo(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.CuckooStatus())

        self.add_action(actions.DeleteTask())

        self.add_action(actions.Exit())

        self.add_action(actions.GetFile())

        self.add_action(actions.GetMemory())

        self.add_action(actions.GetPcap())

        self.add_action(actions.GetReport())

        self.add_action(actions.GetScreenshots())

        self.add_action(actions.ListMachines())

        self.add_action(actions.ListMemory())

        self.add_action(actions.ListTasks())

        self.add_action(actions.RebootTask())

        self.add_action(actions.RerunReport())

        self.add_action(actions.RescheduleTask())

        self.add_action(actions.SubmitFiles())

        self.add_action(actions.SubmitUrl())

        self.add_action(actions.ViewFile())

        self.add_action(actions.ViewMachine())

        self.add_action(actions.ViewTask())

        self.add_action(actions.VpnStatus())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONCuckoo())
    cli.run()


if __name__ == "__main__":
    main()
