Version = '1.0.2'
Description = 'With the Try Bro plugin for Rapid7 InsightConnect, users can use a free instance of Bro Network Security Monitor in the cloud. Users can upload PCAP files for analysis and get Bro logs'


class ICONTryBro(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Files())

        self.add_action(actions.Run())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONTryBro())
    cli.run()


