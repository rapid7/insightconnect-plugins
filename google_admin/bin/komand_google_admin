#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_google_admin import connection, actions, triggers


Name = 'Google Apps Admin'
Vendor = 'rapid7'
Version = '1.0.3'
Description = 'Get and suspend users with the Google admin plugin'


class ICONGoogleAdmin(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.GetUser())

        self.add_action(actions.SuspendUser())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONGoogleAdmin())
    cli.run()


if __name__ == "__main__":
    main()
