import komand
from komand_whois import connection, actions, triggers


Name = 'Whois'
Vendor = 'rapid7'
Version = '1.0.4'
Description = 'Whois Lookups'


class ICONWhois(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Address())

        self.add_action(actions.Domain())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONWhois())
    cli.run()


