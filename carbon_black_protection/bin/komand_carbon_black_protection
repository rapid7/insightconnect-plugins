#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_carbon_black_protection import connection, actions, triggers


Name = 'Carbon Black Protection'
Vendor = 'rapid7'
Version = '2.2.0'
Description = 'Cb Protection allows users to lock down and harden systems to prevent malware, ransomware, and more'


class ICONCarbonBlackProtection(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_trigger(triggers.NewApprovalRequest())

        self.add_action(actions.ApproveFileLocally())

        self.add_action(actions.BanFile())

        self.add_action(actions.CreateFileRule())

        self.add_action(actions.GetApprovalRequest())

        self.add_action(actions.GetFileRule())

        self.add_action(actions.ResolveApprovalRequest())

        self.add_action(actions.RetrieveFileCatalogEntry())

        self.add_action(actions.RetrieveFileInstance())

        self.add_action(actions.UnapproveFileLocally())

        self.add_action(actions.UnbanFile())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONCarbonBlackProtection())
    cli.run()


if __name__ == "__main__":
    main()
