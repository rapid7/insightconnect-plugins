#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_presto import connection, actions, triggers


Name = 'Presto'
Vendor = 'rapid7'
Version = '1.0.2'
Description = 'The Presto plugin will allow you to run database commands with Presto'


class ICONPresto(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Execute())

        self.add_action(actions.ExecuteOne())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONPresto())
    cli.run()


if __name__ == "__main__":
    main()
