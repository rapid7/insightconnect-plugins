#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_networktotal import connection, actions, triggers


Name = 'Network Total'
Vendor = 'rapid7'
Version = '1.0.0'
Description = 'Networktotal is used to upload a PCAP for analysis'


class ICONNetworktotal(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Search())

        self.add_action(actions.Upload())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONNetworktotal())
    cli.run()


if __name__ == "__main__":
    main()
