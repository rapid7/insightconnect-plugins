import komand
from komand_facebook_threat_exchange import connection, actions, triggers


Name = 'Facebook Threat Exchange'
Vendor = 'rapid7'
Version = '1.0.2'
Description = 'A platform for sharing threat information between selected entities'


class ICONFacebookThreatExchange(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.SubmitDescriptors())

        self.add_action(actions.ThreatDescriptors())

        self.add_action(actions.ThreatIndicators())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONFacebookThreatExchange())
    cli.run()


if __name__ == "__main__":
    main()
