#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_logstash import connection, actions, triggers


Name = 'Logstash'
Vendor = 'rapid7'
Version = '1.0.1'
Description = 'Data collection engine with real-time pipelining capabilities'


class ICONLogstash(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.GeneralInfo())

        self.add_action(actions.HotThreads())

        self.add_action(actions.Node())

        self.add_action(actions.NodeStats())

        self.add_action(actions.Plugins())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONLogstash())
    cli.run()


if __name__ == "__main__":
    main()
