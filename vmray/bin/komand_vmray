#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_vmray import connection, actions, triggers


Name = 'VMRay Sandbox'
Vendor = 'rapid7'
Version = '4.0.2'
Description = 'The VMRay plugin allows you to analyze and detect malware before it lands'


class ICONVmray(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.GetAnalysis())

        self.add_action(actions.GetSamples())

        self.add_action(actions.SubmitFile())

        self.add_action(actions.SubmitUrl())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONVmray())
    cli.run()


if __name__ == "__main__":
    main()
