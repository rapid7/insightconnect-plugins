#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_hashit import connection, actions, triggers


Name = 'HashIt'
Vendor = 'rapid7'
Version = '2.0.2'
Description = 'Generate Common Hashes from a File or String'


class ICONHashit(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Bytes())

        self.add_action(actions.String())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONHashit())
    cli.run()


if __name__ == "__main__":
    main()
